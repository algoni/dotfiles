{
        "common": {
                "log_file": "~/powerlog",
                "log_level": "DEBUG"
        },
	"segments": {
		"right": [
			{
				"function": "powerline.segments.common.players.google_play_apple_script",
                                "name": "player",
                                "args": {
                                        "format": "{state_symbol} {artist} - {title}"
                                }
			},
			{
				"function": "powerline.segments.common.time.date"
			},
			{
				"function": "powerline.segments.common.time.date",
				"args": {
					"format": "%H:%M",
					"istime": true
				}
			}
		]
	}
}
